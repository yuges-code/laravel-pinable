{
    "name": "yuges-code/pinable",
    "description": "ðŸ“Œ Package for easily attaching pins to Laravel eloquent models",
    "keywords": [
        "php",
        "pins",
        "yuges",
        "laravel",
        "package"
    ],
    "homepage": "https://yuges-code.github.io/laravel-pinable",
    "support": {
        "source": "https://github.com/yuges-code/laravel-pinable",
        "issues": "https://github.com/yuges-code/laravel-pinable/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Georgy",
            "email": "goshasafonov@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "yuges-code/laravel-package": "^1.1",
        "yuges-code/laravel-orderable": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Yuges\\Pinable\\": "src",
            "Yuges\\Pinable\\Database\\Seeders\\": "database/seeders/",
            "Yuges\\Pinable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuges\\Pinable\\Tests\\": "tests/src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yuges\\Pinable\\Providers\\PinableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
